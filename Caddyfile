{
  {$FRACTURING_SPACE_CADDY_EMAIL}
  auto_https {$FRACTURING_SPACE_CADDY_AUTO_HTTPS}
}

{$FRACTURING_SPACE_CADDY_SITE_PREFIX}auth.{$FRACTURING_SPACE_DOMAIN} {
  reverse_proxy auth:8084
}

{$FRACTURING_SPACE_CADDY_SITE_PREFIX}{$FRACTURING_SPACE_DOMAIN} {
  reverse_proxy web:8086
}

{$FRACTURING_SPACE_CADDY_SITE_PREFIX}admin.{$FRACTURING_SPACE_DOMAIN} {
  reverse_proxy admin:8082
}

{$FRACTURING_SPACE_CADDY_SITE_PREFIX}mcp.{$FRACTURING_SPACE_DOMAIN} {
  reverse_proxy mcp:8081
}
